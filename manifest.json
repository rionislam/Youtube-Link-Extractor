{
  "manifest_version": 2,
  "name": "YouTube Link Extractor",
  "version": "1.0.1.1",
  "description": "This helps to extract links of embedded youtube videos from any webpage",
  "permissions": [
    "tabs",
    "activeTab",
    "webRequest",
    "webRequestBlocking"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "assets/popup.png"
  }
}
